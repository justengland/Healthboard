<!-- http://embeddedjs.com/ - check out http://blog.nodejitsu.com/micro-templates-are-dead -->
<h1><%= title %></h1>
<p>Welcome to <%= title %></p>
<p>Total Count: <%= totalCount  %></p>
<style>
table{
    table-layout: fixed;
}
td {
    white-space:nowrap;
    overflow:hidden;
    text-overflow: ellipsis;
    border-collapse:collapse;    
}
</style>
<table border=1>
<tr>
    <td>Log</td>
    <td>Elapsed Time</td>
	<td>Time Spent</td>
    <td>timeStamp</td>
    <td>Message</td>
</tr>
<% for(var i=0; i<records.length; i++) {%>
<tr>
    <td><a href="https://healthgrades.loggly.com/api/search?q=json.requestId:<%= records[i].requestId %>" title='Loggly Request' target='_blank'>view <%= i %></a></td>
    <td><%= records[i].elapsedTime %></td>
	<td><%= records[i].timeSinceLast %></td>
    <td><%= records[i].timeStamp %></td>
    <td><%= records[i].messageType %> :: <%= records[i].message %></td>    
</tr>
<% } %>